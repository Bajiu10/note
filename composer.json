{
    "name": "max/note",
    "description": "一款基于MaxPHP+Swoole的小型博客系统",
    "homepage": "https://github.com/topyao/note",
    "type": "project",
    "license": "Apache-2.0",
    "keywords": [
        "maxphp",
        "flexible",
        "php framework"
    ],
    "authors": [
        {
            "name": "chengyao",
            "email": "987861463@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Lib/helpers.php",
            "app/helpers.php"
        ]
    },
    "require": {
        "php": "^8.0",
        "max/cache": "2.x-dev",
        "max/database": "2.x-dev",
        "max/console": "2.x-dev",
        "max/view": "2.x-dev",
        "max/validator": "2.x-dev",
        "max/log": "2.x-dev",
        "max/routing": "2.x-dev",
        "ext-curl": "*",
        "max/config": "2.x-dev",
        "max/env": "2.x-dev",
        "max/session": "2.x-dev",
        "max/server": "2.x-dev",
        "tencentcloud/tencentcloud-sdk-php": "^3.0",
        "tencentcloud/cvm": "^3.0",
        "swoole/ide-helper": "^4.8"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php max vendor:publish"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.example.env', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
